<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="pettycash">
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.junit.JUnit' />


	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<inherits name='com.google.gwt.resources.Resources' />
	<inherits name='com.sencha.gxt.ui.GXT' />
	
	

	<!-- Load dynamically predefined styles in the library when the application 
		starts -->
	<stylesheet src="Upload.css" />
	

	<!-- specify supported browsers -->
	<set-property name='user.agent' value='ie9,gecko1_8,safari' />

	<!-- specify the app entry point -->
	<entry-point class='org.datacare.pettycash.client.PettyCash' />

	<!-- specify paths for translatable code -->

	<source path='client' />
	<source path='model' />
	<source path='shared' />

</module>